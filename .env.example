# AWS Configuration
AWS_REGION=us-east-1
S3_BUCKET=your-bucket-name
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key

# S3 Source Paths (folder names in S3 bucket)
SOURCEPATH=dev_orders
PRODUCT_CATALOG_SOURCEPATH=dev_product_catalog
DENTAL_PRACTICES_SOURCEPATH=dev_dental_practices
DENTAL_GROUPS_SOURCEPATH=dev_dental_groups
LAB_PRODUCT_MAPPING_SOURCEPATH=dev_lab_product_mapping
LAB_PRACTICE_MAPPING_SOURCEPATH=dev_lab_practice_mapping
PRODUCT_LAB_MARKUP_SOURCEPATH=dev_product_lab_markup
PRODUCT_LAB_REV_SHARE_SOURCEPATH=dev_product_lab_rev_share

# Database Configuration
DB_HOST=your-database-host
DB_PORT=5432
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_NAME=your_db_name
DB_SSL=true

# Logger
LOG_TO_CONSOLE=true
LOG_LEVEL=info

# Salesforce Configuration (for extractors)
SF_LOGIN_URL=https://incisive.my.salesforce.com
SF_CLIENT_ID=<your-connected-app-client-id>
SF_USERNAME=<your-sf-username>
SF_PRIVATE_KEY_PATH=/path/to/sf_jwt.key
# Alternative: provide key content directly (for Docker)
# SF_PRIVATE_KEY=-----BEGIN RSA PRIVATE KEY-----\n...\n-----END RSA PRIVATE KEY-----
SF_API_VERSION=59.0
